<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="root" alignment="center" prefHeight="424.0" prefWidth="600.0" spacing="15.0" stylesheets="@style.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.12" fx:controller="org.grupp2.sdpproject.GUI.customer.RentalHistoryScene">
    <Label fx:id="titleLabel" style="-fx-font-size: 18px; -fx-font-weight: bold;" text="Dina uthyrningar" />

    <ListView fx:id="rentalHistoryList" />

    <HBox spacing="10" alignment="center">
        <Button fx:id="returnButton" onAction="#handleReturn" styleClass="bigbutton" text="Återlämna film" disable="true" />
        <Button fx:id="backButton" onAction="#handleBack" styleClass="bigbutton" text="Bakåt" />
    </HBox>

    <padding>
        <Insets bottom="15.0" left="30.0" right="30.0" top="15.0" />
    </padding>
</VBox>